{
    "version": "2.0",
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        }
      }
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[3.*, 4.0.0)"
    },
    "functionTimeout": "00:10:00",
    "extensions": {
      "durableTask": {
          "maxConcurrentOrchestratorFunctions": 8,
          "maxConcurrentActivityFunctions": 8
      }
    }
  }